			"require": "./dist/getLogFilter.js",
			"types": "./dist/getLogFilter.d.ts"
		},
		"./loadConfigFile": {
			"default": "./dist/loadConfigFile.js",
			"require": "./dist/loadConfigFile.js",
			"types": "./dist/loadConfigFile.d.ts"
		},
		"./parseAst": {
			"import": "./dist/es/parseAst.js",
			"require": "./dist/parseAst.js",
